<Patch>

	<!-- AirCon -->

	<Operation Class="PatchOperationFindMod">
	<mods>
		<li>Dubs Bad Hygiene</li>
	</mods>
		<match Class="PatchOperationSequence">
			<operations>
			
			<li Class="PatchOperationAdd">
				<xpath>Defs/ThingDef[defName="AirConOutdoorUnit"]</xpath>
				<value>
					<building>
						<buildingTags>
							<li>CelsiusSK_EveryBuildingCellPlusOne</li>
						</buildingTags>
					</building>
				</value>
			</li>

			</operations>
		</match>
	</Operation>

</Patch>
